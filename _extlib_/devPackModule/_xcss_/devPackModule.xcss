<?xml version="1.0" encoding="utf-8"?>
<XCSS version="1.0"><![CDATA[.Button.btn_WF_Drop
{
	-nexa-border : 0px none;
	-nexa-padding : 0px;
	background : url("imagerc::devPackModule/cbo_WF_Drop.png") no-repeat center center transparent;
	color : #333333;
	font : 13px "Malgun Gothic";
}
.Button.btn_WF_Drop[status=mouseover],.Button.btn_WF_Drop[userstatus=pushed],.Button.btn_WF_Drop[userstatus=selected]
{
	-nexa-border : 0px none;
	background : url("imagerc::devPackModule/cbo_WF_DropO.png") no-repeat center center transparent;
}
