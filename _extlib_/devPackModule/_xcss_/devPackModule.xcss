{
	font : bold 14px "Malgun Gothic";
}
.Edit.edt_WF_Cbo,.Edit.edt_WF_Cbo[status=readonly]
{
	-nexa-border : 1px solid #dddddd;
	-nexa-padding : 0px 28px 0px 8px;
	background : #ffffff;
